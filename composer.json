{
    "name": "mvaliolahi/request-hydrator",
    "description": "Laravel Request Hydrator Package.",
    "authors": [
        {
            "name": "Meysam Valiolahi",
            "email": "mvaliolahi@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mvaliolahi\\RequestHydrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "mvaliolahi/hydrate": "1.0.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvaliolahi\\RequestHydrator\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0.4",
        "laravel/framework": "^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
